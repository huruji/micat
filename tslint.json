{
  "defaultSeverity": "error",
  "extends": [
      "tslint-config-airbnb",
      "tslint-react",
      "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
      "jsx-alignment": [0],
      "jsx-no-lambda": [0],
      "max-line-length": [true, 120],
      "trailing-comma": false,
      "ter-indent": [
          true,
          4,
          {
              "SwitchCase": 1
          }
      ],
      "variable-name": false,
      "jsx-alignment": true,
      "jsx-equals-spacing": [true, "never"],
      "jsx-no-string-ref": true,
      "jsx-self-close": true
  },
  "rulesDirectory": []
}
