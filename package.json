    "classname": "0.0.0",
    "classnames": "^2.2.6",
    "constate": "^1.2.0",
    "download": "^7.1.0",
    "electron-unhandled": "^3.0.0",
    "eventemitter3": "^4.0.0",
    "events": "^3.0.0",
    "fs-extra": "^8.1.0",
    "image-download": "^1.2.0",
    "image-type": "^4.1.0",
    "menubar": "^6.0.7",
    "platform-folders": "^0.4.1",
    "pretty-checkbox": "^3.0.3",
    "rc-select": "^10.0.0-alpha.11",
