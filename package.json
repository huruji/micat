    "react-pose": "^4.0.8",
    "react-radio-buttons": "^1.2.2",
    "react-redux": "^7.0.3",
    "react-select": "^3.0.4",
    "react-svg-inline": "^2.1.1",
    "react-switch": "^5.0.1",
